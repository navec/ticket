{
	"name": "auth-service",
	"version": "1.0.0",
	"keywords": [],
	"author": "",
	"license": "ISC",
	"description": "",
	"main": "dist/main.js",
	"types": "dist/main.d.ts",
	"scripts": {
		"prebuild": "rm -rf dist && rm -f tsconfig.tsbuildinfo",
		"build": "tsc",
		"postbuild": "tsc-alias",
		"clean": "rm -rf dist",
		"test": "jest",
		"test:unit": "jest src/**",
		"test:e2e": "jest e2e/**",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"lint": "eslint . --fix --max-warnings=0  --ignore-path ../.eslintignore",
		"format": "prettier . --write",
		"dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only  --env-file=.env src/main.ts",
		"start": "node dist/main.js"
	},
	"dependencies": {
		"google-auth-library": "^9.15.1",
		"@ticket/core": "*"
	}
}
