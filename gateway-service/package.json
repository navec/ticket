{
	"name": "gateway-service",
	"version": "1.0.0",
	"keywords": [],
	"author": "",
	"license": "ISC",
	"description": "",
	"main": "dist/main.js",
	"types": "dist/main.d.ts",
	"scripts": {
		"prebuild": "rm -rf dist && rm -f tsconfig.tsbuildinfo",
		"build": "tsc",
		"postbuild": "tsc-alias",
		"clean": "rm -rf dist",
		"test": "cd .. && npm run test core/",
		"test:watch": "cd .. && npm run test:watch core/",
		"test:coverage": "cd .. && npm run test:coverage core/",
		"lint": "eslint . --fix --max-warnings=0  --ignore-path ../.eslintignore",
		"format": "prettier . --write",
		"dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only  --env-file=.env src/main.ts",
		"start": "ts-node --project ./tsconfig.json -r tsconfig-paths/register dist/main.js"
	},
	"dependencies": {},
	"devDependencies": {}
}
